{
  "name": "@jchiam/eslint-plugin-css-in-js",
  "version": "0.0.1",
  "description": "A CSS-in-JS plugin for ESLint.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf ./lib && tsc",
    "prepare": "is-ci || husky install && npm run test && npm run lint && npm run build",
    "test": "jest",
    "lint": "eslint ./src --ext .ts"
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jchiam/eslint-plugin-css-in-js.git"
  },
  "bugs": {
    "url": "https://github.com/jchiam/eslint-plugin-css-in-js/issues"
  },
  "homepage": "https://github.com/jchiam/eslint-plugin-css-in-js#readme",
  "devDependencies": {
    "@jchiam/eslint-config": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@typescript-eslint/utils": "^5.33.1",
    "codecov": "^3.8.3",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8"
  },
  "peerDependencies": {
